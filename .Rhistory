sens <- 0.9652
spec <- 0.9968
p_pos_given_N <- 1 - spec
# Sequence of incidence rates (0% to 5%)
pC <- seq(0, 0.05, by = 0.0001)
# Compute P(C|+)
P_C_given_pos <- (sens * pC) / (sens * pC + p_pos_given_N * (1 - pC))
# Plot
plot(pC * 100, P_C_given_pos * 100, type = "l",
xlab = "Incidence P(C) [%]",
ylab = "Diagnostic power P(C|+) [%]",
main = "Diagnostic Power of Roche RAT 2.0")
